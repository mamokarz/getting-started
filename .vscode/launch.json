    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
            "executable": "./STMicroelectronics/STM32L4_L4+/build/app/stm32l475_azure_iot.bin",
            "name": "Debug Cortex - jlink",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "jlink"
        },

        {
            "cwd": "${workspaceRoot}",
            "executable": "./STMicroelectronics/STM32L4_L4+/build/app/stm32l475_azure_iot.elf",
            "name": "Debug Cortex - stlink",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "stlink"
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "enter program name, for example ${workspaceFolder}/a.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/path/to/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}